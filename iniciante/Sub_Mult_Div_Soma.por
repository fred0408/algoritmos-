programa {
  funcao inicio() {

   inteiro n1, n2, mult, soma, sub, div,
   inteiro n1_mult, n1_soma, n1_sub, n1_div

   //Entrada
   escreva("DIGITE O NUMERO ")
   leia(n1)
   escreva("DIGITE O NUMERO ")
   leia(n2)

   //Processo
   n1_soma = n1 + n2
   n1_mult = n1 * n2
   n1_sub = n1 - n2
   n1_div = n1 / n2

   //Saida
   escreva("A SUBTRACAO É, ", n1_sub, " A SOMA É, ", n1_soma, " A MULTIPLICACAO É, ", n1_mult, " A DIVISAO É, ", n1_div)


    
  }
}
